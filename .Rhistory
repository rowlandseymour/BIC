multivariate.normal.density <- function(x, mu, sigma){
k <- dim(sigma)
sigma.inv <- solve(sigma)
sq.root.terms <- (2*pi)^(-k/2)*det(sigma)^(-k/2)
exponent.term <- -0.5*t(x-mu)%*%solve(sigma)%*%(x-mu)
result <- sq.root.terms*exp(exponent.term)
return(result)
}
