i <- first.item
success <- 0
j <- 1
while(success == 0 & j <= length(ordering)){
if(j == 1){
temp <- c(i, ordering)
} else{
temp <- c(ordering[1:(j-1)], i, ordering[-c(1:(j-1))])
}
